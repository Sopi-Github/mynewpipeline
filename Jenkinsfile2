pipeline
    {
	  agent any
	  stages
	     {
		   stage ('scm checkout')
		     {
			   steps {
						git branch: 'master', url: 'https://github.com/Sopi-Github/mynewpipeline.git'
				     }
			  }
		   stage ('gradle build')
		   {
		       steps {
			        withGradle(jdk: 'localjdk-1.8', gradle: 'localgradle') {
                    sh 'gradle build'
				}			   
			   }
		   }
		 }
    }
